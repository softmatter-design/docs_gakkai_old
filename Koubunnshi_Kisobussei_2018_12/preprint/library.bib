title = {{Statistical Theory of Networks of Non-Gaussian Flexible Chains}},
url = {http://aip.scitation.org/doi/10.1063/1.1700682 http://scitation.aip.org/content/aip/journal/jcp/20/7/10.1063/1.1700682},
