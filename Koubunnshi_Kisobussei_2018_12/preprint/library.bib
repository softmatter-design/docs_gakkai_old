isbn = {0024-9297},
issn = {15205835},
journal = {Macromolecules},
month = {jan},
number = {1},
pages = {344--352},
publisher = {American Chemical Society},
title = {{Nearly Ideal Polymer Network Ion Gel Prepared in pH-Buffering Ionic Liquid}},
url = {http://pubs.acs.org/doi/abs/10.1021/acs.macromol.5b02360},
volume = {49},
year = {2016}
