journal = {J. Elast.},
keywords = {Constitutive equations,Full-network models,Non-Gaussian chains,Rubber elasticity},
number = {1-3},
pages = {65--86},
publisher = {Kluwer Academic Publishers},
title = {{An average-stretch full-network model for rubber elasticity}},
url = {http://link.springer.com/10.1023/B:ELAS.0000005553.38563.91},
volume = {70},
