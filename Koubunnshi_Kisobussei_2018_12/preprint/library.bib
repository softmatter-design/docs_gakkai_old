doi = {10.1016/0022-5096(93)90043-F},
file = {:C$\backslash$:/Users/sasaki/Documents/Mendeley Desktop/Wu, Van Der Giessen/Journal of the Mechanics and Physics of Solids/Wu, Van Der Giessen - 1993 - On improved network models for rubber elasticity and their applications to orientation hardening in glassy.pdf:pdf},
