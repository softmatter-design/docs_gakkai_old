file = {:C$\backslash$:/Users/sasaki/Documents/Mendeley Desktop/Auhl et al/Journal of Chemical Physics/Auhl et al. - 2003 - Equilibration of long chain polymer melts in computer simulations.pdf:pdf},
isbn = {0021-9606},
issn = {00219606},
journal = {J. Chem. Phys.},
month = {dec},
