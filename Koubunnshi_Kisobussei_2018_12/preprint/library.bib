volume = {125},
year = {2012}
}
@article{Fukasawa2010,
abstract = {Tetra-poly(ethylene glycol) (PEG)-based nanocomposite hydrogels (tP-NC$\backslash$ngels) were prepared by in situ polymerization of two kinds of macromonomer$\backslash$n(TN-PEG and TA-PEG), each having four reactive PEG arms, in the presence$\backslash$nof clay (Laponite XLG) in aqueous media. By adopting appropriate$\backslash$nsynthetic conditions such as the use of pyrophosphate-Na buffer and$\backslash$nselected homogeneous mixing procedures, tP-NC gels with high transparency$\backslash$nand excellent tensile properties, e.g., high elongation (900-1000{\%})$\backslash$nand high tensile strength (300-560 kPa), approximately 2-4 times$\backslash$nthose of the corresponding original tetra-PEG gel, were obtained$\backslash$nat a relatively low clay concentration (C clay = 2×10 -2 mol/L-H$\backslash$n2O) and a wide range of polymer concentrations (C p = 120-240 mg/mL-H$\backslash$n2O). We investigated the effects of buffer, C clay, and C p on forming$\backslash$ntetra-PEG/clay gels, their tensile properties, and the network structures.$\backslash$nAlso, we studied interactions between the clay and tetra-PEG segments,$\backslash$nby measurements such as viscometry, X-ray fluorescence, transmission$\backslash$nelectron microscopy, and Fourier transfer infrared spectroscopy.$\backslash$nIt was revealed that the pyrophosphate-Na buffer plays an important$\backslash$nrole in dispersing exfoliated clay uniformly in the reaction solution$\backslash$nand that the clay platelets incorporated into the tetra-PEG networks$\backslash$ninteract with a specific ester group in the tetra-PEG arm as well$\backslash$nas with amide linkages in the tetra-PEG networks so as to improve$\backslash$ntheir tensile properties. {\textcopyright} 2010 American Chemical Society.},
