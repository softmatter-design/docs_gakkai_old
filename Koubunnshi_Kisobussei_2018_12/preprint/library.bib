journal = {Int. J. Plast.},
keywords = {A. Thermomechanical processes,B. Constitutive behaviour,B. Elastic-viscoplastic material,B. Polymeric material,Disentanglement/slip-link},
month = {apr},
pages = {102--126},
publisher = {Pergamon},
title = {{A thermo-mechanical large deformation constitutive model for polymers based on material network description: Application to a semi-crystalline polyamide 66}},
