doi = {10.1021/ma970364k},
file = {:C$\backslash$:/Users/sasaki/Documents/Mendeley Desktop/Rubinstein, Panyukov/Macromolecules/Rubinstein, Panyukov - 1997 - Nonaffine deformation and elasticity of polymer networks.pdf:pdf},
