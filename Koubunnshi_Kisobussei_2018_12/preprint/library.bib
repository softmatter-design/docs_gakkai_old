issn = {00225096},
journal = {J. Mech. Phys. Solids},
keywords = {Constitutive behavior,Polymeric material,Residual stress},
pages = {110--125},
publisher = {Elsevier},
title = {{Disorder, pre-stress and non-affinity in polymer 8-chain models}},
url = {http://dx.doi.org/10.1016/j.jmps.2016.01.014},
volume = {89},
year = {2016}
}
