publisher = {Springer Berlin Heidelberg},
title = {{Reduced stress concentration and enhanced fracture toughness by yielding-rehardening combination}},
url = {http://link.springer.com/10.1140/epje/i2012-12023-0},
volume = {35},
year = {2012}
}
